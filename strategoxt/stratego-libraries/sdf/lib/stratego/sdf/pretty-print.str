module stratego/sdf/pretty-print
imports
   libstratego-gpp
   stratego/sdf/desugar
   

strategies

  pp-sdf-box =
    sdf-ppfix
    ; ast2box(| [ <pp-table-sdf> ] )

  pp-sdf-string =
    pp-sdf-box
    ; box2text-string(|80)

  pp-sdf-stream(|stream) =
    pp-sdf-box
    ; box2text-stream(|80, stream)

  pp-table-sdf =
    import-term(Sdf2.pp.af)
