module Identifiers
exports
  lexical syntax
    [a-zA-Z\_] [a-zA-Z\_0-9]* -> ID

  lexical restrictions
    ID -/- [a-zA-Z\_0-9]

  context-free syntax
    ID          -> IDENTIFIER  {cons("Id")}
    ID          -> TypedefName {cons("TypeId"), avoid}
    IDENTIFIER  -> IdentifierOrTypedefName
    TypedefName -> IdentifierOrTypedefName

