module parse-box
imports liblib strategoxt-xtc-tools Box

strategies
  
  io-parse-box =
    xtc-io-wrap(
      xtc-sglr(!"Box.tbl")
    ; xtc-implode-asfix
    ; xtc-io-transform(box-desugar)
    )

  box-desugar =
    topdown(
      try(
        S(un-double-quote)
      )
    )

  box-ensugar =
    topdown(
      try(
        S(double-quote)
      )
    )
