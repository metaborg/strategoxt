module char-test
imports libstratego-lib

strategies 

  main-char-test = 
    test-suite(!"char-test",
	is-num-test;
	is-alphanum-test;
	is-not-num-test
    )

  is-num-test =
    apply-test(!"is-num-test",is-num,!50,!50)

  is-alphanum-test =
    apply-test(!"is-alphanum-test",is-alphanum,!50,!50)

  is-not-num-test =
    apply-test(!"is-not-num-test",not(is-num),!130,!130)
