/**
 * Removes all cons(...) attributes from the productions in an Sdf grammar.
 *
 * @author Joost Visser (Joost.Visser@cwi.nl)
 */
module rm-cons
imports liblib Sdf2

strategies

  io-rm-cons =
    io-wrap(RmCons)

  RmCons =
    alltd(attrs(filter(not(?term(cons(_)) + ?cons(_)))))

signature
  constructors
    cons: ATerm -> Attribute
