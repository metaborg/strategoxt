rules

  generic-str-to-abox(|cons,op) =
    let 
      cons-to-list =
      rec x({a,b,c:
        (  ?cons#([a,cons#([b,c])]) ; ![a,b|<x>c]
        <+ ?cons#([a,b]) ; ![a,b]  
        <+ ![<id>] )
      })
    in 
      ?cons#([s1,s2])
    ; worstcase-nicecase(
        where( 
          cons-to-list => [s1'|ss] 
        ; <map(\ s -> |[ H [ ~lit:op ~s ] ]| \)>ss => ss' 
        )
      ; !|[ V is=0 [ ~s1' ~*ss' ] ]|
      , !|[ H [ ~s1 ~lit:op ~s2 ] ]|
      )
    end

  stratego-to-abox-prefer =
    generic-str-to-abox(|"Seq"    , ";")
  + generic-str-to-abox(|"Choice" , "+")
  + generic-str-to-abox(|"LChoice", "<+")
  + generic-str-to-abox(|"RChoice", "+>")

  
